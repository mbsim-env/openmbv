prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@

XSLDIR=@datadir@/@PACKAGE@/xsl
DOCDIR=@datadir@/@PACKAGE@/doc
SCHEMADIR=@datadir@/@PACKAGE@/schema
BINDIR=@prefix@/bin
OCTAVEDIR=@datadir@/@PACKAGE@/octave

Name: MBXMLUtils
Description: XML utilities for physical applications (multi body applications)
Requires: xerces-c
Version: @VERSION@
Libs: -L${libdir} -lmbxmlutils @MKOCTFILE_LIBS@ @CASADI_LIBS@ -l@BOOST_SYSTEM_LIB@ -l@BOOST_FILESYSTEM_LIB@
Cflags: -I${includedir} @MKOCTFILE_CFLAGS@ @CASADI_CPPFLAGS@

